<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>

<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml">
    <VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <Label text="Edit Profile">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                <children>
                    <Label layoutX="95.0" layoutY="14.0" text="Name:">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <Label layoutX="17.0" layoutY="54.0" text="Starting Weight:">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <Label layoutX="43.0" layoutY="93.0" text="Goal Weight:">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <TextField fx:id="nameTextField" layoutX="160.0" layoutY="15.0" />
                    <TextField fx:id="startTextField" layoutX="160.0" layoutY="55.0" />
                    <TextField fx:id="limitTextField" layoutX="160.0" layoutY="94.0" />
                    <Button layoutX="241.0" layoutY="191.0" prefWidth="70.0" text="Save" onAction="#handleSaveButton"/>
                    <Button layoutX="148.0" layoutY="191.0" prefWidth="70.0" text="Cancel" onAction="#handleCancelButton"/>
                    <TextField layoutX="162.0" layoutY="130.0" />
                    <Label layoutY="128.0" text="Daily Calorie Limit:">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                </children>
            </AnchorPane>
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </VBox>
</fx:root>