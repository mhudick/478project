<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane fx:id="flowpane" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.CreateUserController"
        hgap="10"
        vgap="10"
        prefHeight="100"
        prefWidth="400"
        alignment="CENTER">
    <Label fx:id="nameLabel" text="Name: "/>

    <TextField fx:id="nameTextField"/>

    <Label fx:id="ageLabel" text="Age: "/>

    <TextField fx:id="ageTextField"/>

    <Button fx:id="createButton" text="Create" onAction="#handleCreateButton"/>
    <Button fx:id="backButton" text="Back" onAction="#handleBackButton"/>
</FlowPane>