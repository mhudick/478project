<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane fx:id="flowpane" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.UserNew"
        hgap="10"
        vgap="10"
        prefHeight="100"
        prefWidth="400"
        alignment="CENTER">
    <Label fx:id="labelName" text="Name: "/>

    <TextField fx:id="fieldName"/>

    <Label fx:id="labelAge" text="Age: "/>

    <TextField fx:id="fieldAge"/>

    <Button fx:id="createButt" text="Create" onAction="#handleCreateButton"/>
    <Button fx:id="backButt" text="Back" onAction="#handleBackButton"/>
</FlowPane>